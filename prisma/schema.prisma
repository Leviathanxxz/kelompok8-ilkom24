generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}

model Users {
    id       String @id @default(uuid())
    email    String @unique
    password String
}

model Content {
    id          String @id @default(uuid())
    title       String @unique
    description String
}

model AdminUsers {
    id      Int     @id @default(autoincrement())
    nama    String
    address String?
    image   String
    
}
